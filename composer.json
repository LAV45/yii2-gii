{
    "name": "yiisoft/yii2-gii",
    "description": "The Gii extension for the Yii framework",
    "keywords": ["yii2", "gii", "code generator"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-gii/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2-gii"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com"
        },
        {
            "name": "Alex Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "yiisoft/yii2": "2.0.*",
        "yiisoft/yii2-bootstrap4": "1.0.x-dev",
        "phpspec/php-diff": ">=1.0.2",
        "bower-asset/typeahead.js": "0.10.* | ~0.11.0",
        "bower-asset/glyphicons-only-bootstrap": "~0.0.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "yii\\gii\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\gii\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
